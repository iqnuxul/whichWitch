{
  "name": "whichwitch",
  "version": "1.0.0",
  "description": "Web3 Creation Platform - NFT creation and derivative works management",
  "private": true,
  "type": "module",
  "scripts": {
    "compile": "hardhat compile",
    "test": "hardhat test",
    "deploy": "hardhat run scripts/deploy.js",
    "deploy:sepolia": "hardhat run scripts/deploy.js --network sepolia",
    "deploy:zeta": "hardhat run scripts/deploy-zeta.js --network zeta_testnet",
    "node": "hardhat node",
    "clean": "hardhat clean",
    "backend": "cd src/backend && node server.js",
    "backend:dev": "cd src/backend && nodemon server.js",
    "frontend": "cd src/ui && npm run dev",
    "frontend:build": "cd src/ui && npm run build",
    "frontend:start": "cd src/ui && npm start",
    "dev": "concurrently \"npm run backend:dev\" \"npm run frontend\"",
    "install:all": "npm install && cd src/ui && npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iqnuxul/whichWitch.git"
  },
  "keywords": ["web3", "nft", "blockchain", "ethereum", "next.js"],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/iqnuxul/whichWitch/issues"
  },
  "homepage": "https://github.com/iqnuxul/whichWitch#readme",
  "dependencies": {
    "@supabase/supabase-js": "^2.45.7",
    "axios": "^1.6.2",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "ethers": "^6.15.0",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.9.7"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^2.1.0",
    "@nomicfoundation/hardhat-ethers": "^3.1.2",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.11",
    "@nomicfoundation/hardhat-toolbox": "^5.0.0",
    "@nomicfoundation/hardhat-verify": "^2.0.11",
    "@openzeppelin/contracts": "^5.4.0",
    "@typechain/ethers-v6": "^0.5.1",
    "@typechain/hardhat": "^9.1.0",
    "@types/node": "^24.10.1",
    "chai": "^4.3.10",
    "hardhat": "^2.22.0",
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
